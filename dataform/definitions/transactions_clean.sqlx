config {
  type: "table",
  tags: ["cleaning"]
}

select
  step,
  type,
  amount,
  nameOrig,
  oldbalanceOrg,
  newbalanceOrig,
  nameDest,
  oldbalanceDest,
  newbalanceDest,
  isFraud,
  isFlaggedFraud
from
  `${var("source_dataset")}.${var("transactions_table")}`
where
  amount > 0
  and oldbalanceOrg >= 0
  and newbalanceOrig >= 0
